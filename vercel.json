{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/template(\\d+).html",
      "dest": "/template$1.html"
    }
  ],
  "env": {
    "GROQ_API_KEY": "@groq_api_key"
  }
}
